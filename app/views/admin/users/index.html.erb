<h1><i class="fas fa-fan"></i>会員一覧</h1>
<% @users.each do |user| %>
<table>
  <tr>
    <td><%= image_tag user.get_profile_image(40,40) %></td>
    <td>
      <%= link_to admin_user_path(user.id) do %>
        <%= user.nickname %>
      <%end%>
      （<%= user.posts.count %>件の投稿）
    </td>
    <td>会員ステータス:
      <% if user.is_deleted==false %>
        有効
      <% else %>
        退会
      <% end %>
    </td>
    <td><%#= user.introduction %></td>
  </tr>
</table>
<%end%>